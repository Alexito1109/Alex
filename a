    }                                        

    private void txtNombreActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
    }                                         

    private void btnRegclienteActionPerformed(java.awt.event.ActionEvent evt) {                                              
    
        nombre=txtNombre.getText();
        cedula=txtCedula.getText();
        celular=txtCelular.getText();
        correo=txtCorreo.getText();
        JOptionPane.showMessageDialog(rootPane, "Datos guardados");
    }                                             

    private void cmbArticulosItemStateChanged(java.awt.event.ItemEvent evt) {                                              
        // Cargar marca segun dispositivo seleccionado:
        if(evt.getStateChange()==ItemEvent.SELECTED){
            if(this.cmbArticulos.getSelectedIndex()>0){
                this.cmbMarca.setModel(new DefaultComboBoxModel(this.implementos(this.cmbArticulos.getSelectedItem().toString())));
                
            }
            
        }
    }                                             

    private void cmbMarcaItemStateChanged(java.awt.event.ItemEvent evt) {                                          
        // TODO add your handling code here:
        if(evt.getStateChange()==ItemEvent.SELECTED){
            if(this.cmbMarca.getSelectedIndex()>0){
                this.cmbColor.setModel(new DefaultComboBoxModel(this.color(this.cmbMarca.getSelectedItem().toString())));
                
            }
            
        }
    }                                         

    private void cmbColorItemStateChanged(java.awt.event.ItemEvent evt) {                                          
        // TODO add your handling code here:
        if(evt.getStateChange()==ItemEvent.SELECTED){
            if(this.cmbColor.getSelectedIndex()>0){
                this.cmbTalla.setModel(new DefaultComboBoxModel(this.talla(this.cmbColor.getSelectedItem().toString())));
                
            }
            
        }
    }                                         

    private void cmbColorActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
    }                                        

    private void btnFacturarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        txtaFactura.setText("::::::::::::::::::F A C T U R A:::::::::::::::::"
        +"\n::::::::::::::::DATOS DEL CLIENTE::::::::::::::::::::::::::"
        +"\nCliente:"+nombre
        +"\nCedula:"+cedula
        +"\nCelular:"+celular
        +"\nColor"+correo
        +"\n::::::::::::::::DATOS DE LA COMPRA:::::::::::::::::::::::::"
        +"\nArticulo "+" \nCantidad "+" \nMarca "+" \nColor "+" \nTalla "+" \nP.Unitario "+" \nTotal"
        +"\n   "+articulos +""+cantidad +""
        );
    }                                           

