 public String[] implementos (String implementos){
     //areglo tipo String
     String [] impl=new String [5];
     if(implementos.equalsIgnoreCase("Pupos")){
         impl[0]="Seleccione";
         impl[1]="Adidas";
         impl[2]="Puma";
         impl[3]="Nike";
         impl[4]="Raptor";
//         impl[5]="Mercurial";
         
     }
     if(implementos.equalsIgnoreCase("Canilleras")){
         impl[0]="Mercurial";
         impl[1]="Adidas";
         impl[2]="Puma";
         impl[3]="Nike";
         impl[4]="LM10";
 }
      if(implementos.equalsIgnoreCase("Guantes")){
         impl[0]="Nike";
         impl[1]="UEFA";
         impl[2]="League";
         impl[3]="Supreme";
         impl[4]="Gucci";
 }
       if(implementos.equalsIgnoreCase("Licras")){
         impl[0]="Dlce1";
         impl[1]="Prorunner";
         impl[2]="Giroud"; 
         impl[3]="Puma";
 }
       if(implementos.equalsIgnoreCase("M Recortadas")){
         impl[0]="Bishou";
         impl[1]="Plabyc";
         impl[2]="Spporting"; 
         impl[3]="Kings"; 
 }
        if(implementos.equalsIgnoreCase("Tape")){
         impl[0]="Atlethic";
         impl[1]="Rmp";
         impl[2]="Tape Wrap";  
         impl[3]="Ab Tape"; 
 }
    return impl;       
 }
           
           
