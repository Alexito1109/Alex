    private void btnAceptarActionPerformed(java.awt.event.ActionEvent evt) {                                           
//declarar variables.
    
cantidad=Integer.parseInt(txtCantidad.getText());
producto=cmbArticulos.getSelectedIndex();
marca=cmbMarca.getSelectedItem().toString();
color=cmbColor.getSelectedItem().toString();
talla=cmbTalla.getSelectedItem().toString();
//Metodos. Estructura selecctiva
//switch(marca){
//    case "Seleccione":
//        JOptionPane.showMessageDialog(rootPane,"Seleccione un producto");
//       
//            break;
//    case "Adidas":
//        marca="Adidas";
//        
//        
//        break;
//        case "Nike":
//        marca="Nike";
//        
//        break;
//        case "Puma":
//        marca="Puma";
//        break;
//        
//        default:
//            JOptionPane.showMessageDialog(rootPane,"producto no disponible");
//        
//    
//}txtMarca.setText(""+marca);


switch(producto){
    case 0:
        JOptionPane.showMessageDialog(rootPane,"Seleccione un producto");
       
            break;
    case 1:
        articulos="Pupos";
        pu=30.00;
        total=cantidad*30.00;
        
        break;
        case 2:
        articulos="Canilleras";
        pu=5.00;
        total=cantidad*5.00;
        
        break;
        case 3:
        articulos="Guantes";
        pu=25.00;
        total=cantidad*25.00;
        
        break;
        case 4:
            pu=15.00;
        articulos="Licras";
        total=cantidad*15.00;
        
        break;
        case 5:
            articulos="M Recortadas";
            pu=10.00;
            total=cantidad*10.00;
            break;
            case 6:
            articulos="Tape";
            pu=3.00;
            total=cantidad*3.00;
            break;
        default:
            JOptionPane.showMessageDialog(rootPane,"producto no disponible");
        
    
}
//txtArticulo1.setText(" "+producto);
//txtTotal.setText(" "+total+"$");
cmbArticulos.setSelectedIndex(0);
txtCantidad.setText("");
//txtMarca.setText("");

        // TODO add your handling code here:
    }                                          

    private void btnRegistrotblActionPerformed(java.awt.event.ActionEvent evt) {                                               

reg=(DefaultTableModel)this.tblDatos.getModel();  
reg.insertRow(reg.getRowCount(), new Object[]{nombre,cedula,celular,correo
        ,articulos,cantidad,marca,color,talla,pu,total,
}); 

// TODO add your handling code here:
    }                                              

    private void btnregActionPerformed(java.awt.event.ActionEvent evt) {                                       
        //Declarar variables
//      
 
        txtNombre.setText("");
        txtCedula.setText("");
        txtCelular.setText("");
        txtCorreo.setText("");
    }                                      

