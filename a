     public String[] color (String color){
     //areglo tipo String
     String [] col=new String [5];
     if(color.equalsIgnoreCase("Adidas")){
          col[0]="Seleccione";
          col[1]="Rojos con Negro";
          col[2]="Celeste con negro";
          col[3]="Rosados";
          col[4]="Dorados";
//         impl[5]="Mercurial";
         
     }
     if(color.equalsIgnoreCase("Puma")){
         col[0]="Negros";
